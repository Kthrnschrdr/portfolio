<div class="projects">
  <h1>Projects</h1>
  <% @projects.each do |project| %>
  <div class="each_project">
    <p><%= link_to project.name, project_path(project.id) %></p>
  </div>
  <% end %>
</div>

<% if current_user %>
	<div class="bottom_link"><%= link_to "New Project", new_project_path %></div>
<% end %>