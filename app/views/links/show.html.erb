<h1><%= @link.name %></h1>
<div class="link">
  <h2><%= link_to "Take a Look!", (@link.url) %></h2>
</div>
<% if current_user %>
<div class="bottom_link"><%= link_to "Edit", edit_link_path %></div>
<div class="bottom_link"><%= link_to "Omit", links_path, action: "destroy", id: @link.id %></div>
<% end %>