<h1>Log In</h1>
<div class="form_field">
  <%= form_tag create_login_path do %>
    <%= label_tag :email %>
    <p><%= text_field_tag :email, params[:email] %></p>
    <%= label_tag :password %><br />
    <p><%= password_field_tag :password %></p>
    <div class="action">
      <%= submit_tag "Log In" %>
    </div>
  <% end %>
</div>